{
    "manifest_version": 3,
    "name": "TM Suite",
    "description": "Combined: Distribte + TM Analytics + TM Auto Accept + TM Cleaner",
    "version": "1.0.0",
    "icons": {
        "16": "images/color/icon16.png",
        "48": "images/color/icon48.png",
        "128": "images/color/icon128.png"
    },
    "background": {
        "service_worker": "js/background.js"
    },
    "permissions": [
        "storage",
        "tabs",
        "notifications",
        "cookies",
        "nativeMessaging",
        "ds",
        "contentSettings",
        "browsingData",
        "management",
        "alarms",
        "webNavigation",
        "scripting"
    ],
    "host_permissions": [
        "https://api.distribteportal.com/*",
        "https://distribteportal.com/*",
        "https://whoerip.com/*",
        "https://*.whoerip.com/*",
        "<all_urls>"
    ],
    "action": {
        "default_icon": {
            "16": "images/color/icon16.png",
            "48": "images/color/icon48.png",
            "128": "images/color/icon128.png"
        },
        "default_popup": "popup.html"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Alt+Shift+X"
            },
            "description": "Open Distribte and auto-login"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "https://whoerip.com/*",
                "https://*.whoerip.com/*"
            ],
            "js": ["/js/auto-trigger.js"],
            "run_at": "document_end"
        },
        {
            "matches": [
                "https://www.ticketmaster.com/*",
                "https://www.ticketmaster.ca/*",
                "https://www.ticketmaster.com.au/*",
                "https://www.ticketmaster.co.uk/*",

                "https://checkout.ticketmaster.com/*",
                "https://checkout.ticketmaster.ca/*",
                "https://checkout.ticketmaster.com.au/*",
                "https://checkout.ticketmaster.co.uk/*",

                "https://www.livenation.com/*",
                "https://www.livenation.ca/*",
                "https://www.livenation.com.au/*",
                "https://www.livenation.co.uk/*",

                "https://concerts.livenation.com/*",
                "https://concerts.livenation.ca/*",
                "https://concerts.livenation.com.au/*",
                "https://concerts.livenation.co.uk/*",

                "https://checkout.livenation.com/*",
                "https://checkout.livenation.ca/*",
                "https://checkout.livenation.com.au/*",
                "https://checkout.livenation.co.uk/*",

                "https://tix.axs.com/*",
                "https://www.axs.com/*",
                "https://seatgeek.com/*",
                "https://etix.com/*",
                "https://*.etix.com/*",
                "https://mlb.tickets.com/*",
                "https://*.evenue.net/*"
            ],
            "js": ["/js/content.js"],
            "css": ["/css/styles.css"]
        },
        {
            "matches": [
                "https://etix.com/*",
                "https://*.etix.com/*"
            ],
            "js": ["/js/content_etix_inject_scripts.js"],
            "run_at": "document_start"
        },
        {
            "matches": [
                "https://seatgeek.com/*"
            ],
            "js": ["/js/content_sg_inject_scripts.js"],
            "run_at": "document_start"
        },
        {
            "matches": [
                "https://tix.axs.com/*",
                "https://www.axs.com/*"
            ],
            "js": ["/js/content_axs_inject_scripts.js"],
            "run_at": "document_start"
        },
        {
            "matches": [
                "https://checkout.ticketmaster.com/*",
                "https://checkout.ticketmaster.ca/*",
                "https://checkout.ticketmaster.com.au/*",
                "https://checkout.ticketmaster.co.uk/*",

                "https://checkout.livenation.com/*",
                "https://checkout.livenation.ca/*",
                "https://checkout.livenation.com.au/*",
                "https://checkout.livenation.co.uk/*",

                "https://www.ticketmaster.com/checkout/*",
                "https://www.ticketmaster.ca/checkout/*",
                "https://www.ticketmaster.com.au/checkout/*",
                "https://www.ticketmaster.co.uk/checkout/*",

                "https://tix.axs.com/*",
                "https://www.axs.com/*",
                "https://seatgeek.com/*",
                "https://etix.com/*",
                "https://*.etix.com/*",
                "https://mlb.tickets.com/*",
                "https://*.evenue.net/*"
            ],
            "js": ["/js/toast.js"],
            "css": ["/css/styles.css"]
        },
        {
            "matches": [
                "https://auth.ticketmaster.com/*",
                "https://auth.ticketmaster.ca/*",
                "https://auth.ticketmaster.com.au/*",
                "https://auth.ticketmaster.co.uk/*",

                "https://boxoffice.etix.com/ticket/login/*",
                "https://seatgeek.com/*",
                "https://mlb.tickets.com/*"
            ],
            "js": ["/js/parse_service_email.js"],
            "all_frames": true
        },
        {
            "matches": [
                "*://*.ticketmaster.com/*",
                "*://*.ticketmaster.ca/*",
                "*://*.livenation.com/*",
                "*://shop.axs.com/*"
            ],
            "js": ["/js/tm_analytics/content.js"]
        },
        {
            "matches": [
                "*://*.ticketmaster.com/*",
                "*://*.ticketmaster.co.uk/*",
                "*://*.livenation.com/*"
            ],
            "js": ["/js/tm_autoclick/content.js"],
            "run_at": "document_start",
            "all_frames": true
        }
    ],
    "options_ui": {
        "page": "tm_analytics_options.html",
        "open_in_tab": false
    },
    "web_accessible_resources": [
        {
            "resources": ["/js/inject_email_parser.js", "/js/inject_get_digital_data.js"],
            "matches": ["<all_urls>"]
        },
        {
            "resources": ["/js/inject_etix_fetch_listener.js"],
            "matches": ["https://*.etix.com/*", "https://etix.com/*"]
        },
        {
            "resources": ["/js/inject_sg_fetch_listener.js"],
            "matches": ["https://seatgeek.com/*"]
        },
        {
            "resources": ["/js/inject_axs_fetch_listener.js"],
            "matches": ["https://tix.axs.com/*", "https://www.axs.com/*"]
        }
    ],
    "key": "",
    "update_url": "https://distribteportal-public.s3.amazonaws.com/updates.xml"
}
